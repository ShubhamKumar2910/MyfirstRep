public class DataDisplayController {
    public Object conrec {get;set;} 
    public String PDFData {get; set;} 
    public String keyAccomp{get;set;}
	public String exprGap {get; set;} 
    public String arDev {get; set;}
    public String perImg {get; set;} 
    public String relExp {get; set;}
    public String uniqChar {get; set;} 
    public String candidateMot{get;set;}
    public String language{get;set;}

    
    public DataDisplayController(){
        PDFData='';
        exprGap = '';
        arDev = '';
        perImg = '';
        relExp = '';
        uniqChar = '';
        keyAccomp='';
        candidateMot='';
        language='';
        
    }
    
    public PageReference downloadPDF(){
        System.PageReference pageRef = new System.PageReference('/apex/PDFGenerator');
        exprGap=exprGap.replaceAll('\\<.*?\\>', '');
        arDev=arDev.replaceAll('\\<.*?\\>', '');
        perImg=perImg.replaceAll('\\<.*?\\>', '');
        relExp=relExp.replaceAll('\\<.*?\\>', '');
        uniqChar=uniqChar.replaceAll('\\<.*?\\>', '');
        keyAccomp=keyAccomp.replaceAll('\\<.*?\\>', '');
        candidateMot=candidateMot.replaceAll('\\<.*?\\>', '');
                         
        //ensure pdf downloads and is assigned with defined name
        pageRef.getHeaders().put('content-disposition', 'attachment; filename=TestPDF.pdf');
     
        return pageRef;
    }
}
